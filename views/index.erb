<div class="container">
	<% if authorized? %>
	    <h4>asd</h4>
	<% end %>
  <table class="table table-bordered">
  <% @archives.each do |archive| %>
    <tr>
      <td class="file file-<%= archive.file_type %>"><a href="/download/<%= archive.id %>"><%= archive.full_name %></a></td>
      <td><%= archive.teacher.name %></td>
      <td><%= archive.size_in_mb %></td>
      <td><%= archive.downloads %></td>
    </tr>
  <% end %>
</table>
</div>